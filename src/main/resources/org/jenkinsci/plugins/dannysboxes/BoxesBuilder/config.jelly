<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout" xmlns:t="/lib/hudson" xmlns:f="/lib/form">

    <f:entry>
        <f:repeatable items="${instance.boxes ?: descriptor.defaultBoxes}" var="box" name="boxes" noAddButton="true">
            <table width="100%" padding="0">
                <st:include page="config.jelly" class="${descriptor.boxClass}" it="${box}"/>
            </table>
        </f:repeatable>
    </f:entry>

</j:jelly>
